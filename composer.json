{
  "name": "iods/php-framework",
  "version": "0.1.0",
  "description": "A framework for everything past, present, and future PHP.",
  "type": "framework",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Rye Miller",
      "email": "rye@drkstr.dev"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.8.4",
    "phpunit/phpunit": "^10.1"
  },
  "autoload": {
    "psr-4": {
      "Iods\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iods\\Tests\\": "tests/"
    }
  }
}
